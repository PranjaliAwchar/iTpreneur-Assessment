# quick check for invalid ages in a list of dicts
def validate_data(data):
    bad_entries = []
    for row in data:
        age_val = row.get("age")
        # age should be an integer, otherwise mark it invalid
        if not isinstance(age_val, int):
            bad_entries.append(row)
    return bad_entries

sample = [{"name": "Alice", "age": 30}, {"name": "Bob", "age": "25"}]
print("Invalid rows:", validate_data(sample))
