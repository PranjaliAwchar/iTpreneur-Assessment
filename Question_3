import pandas as pd

df = pd.DataFrame({"income": [50000, None, 60000, None, 55000]})

skew = df["income"].skew()
if abs(skew) < 0.5:
    # roughly normal → use median
    fill_val = df["income"].median()
else:
    # skewed → use mode
    fill_val = df["income"].mode()[0]

df["income"].fillna(fill_val, inplace=True)
print(df)
